{
  "kind": "Deployment",
  "id": "webserver",
  "image": "liferaycloud/nginx:1.20.1-4.1.4",
  "memory": 512,
  "cpu": 4,
  "dependencies": [
    "liferay"
  ],
  "readinessProbe": {
    "httpGet": {
      "path": "/nginx_status",
      "port": 80
    },
    "initialDelaySeconds": 30,
    "timeoutSeconds": 10,
    "failureThreshold": 20
  },
  "livenessProbe": {
    "httpGet": {
      "path": "/nginx_status",
      "port": 80
    },
    "initialDelaySeconds": 60,
    "timeoutSeconds": 10,
    "failureThreshold": 10
  },
  "volumes": {
    "data": "/data"
  },
  "environments": {
    "infra": {
      "deploy": false
    },
    "dev": {
      "loadBalancer": {
        "targetPort": 80,
        "cdn": false,
        "customDomains": ["DEV-test.liferay.com"]
      }
    },
    "uat": {
      "loadBalancer": {
        "targetPort": 80,
        "cdn": true,
        "customDomains": ["UAT-test.liferay.com"]
      }
    },
    "prd": {
      "loadBalancer": {
        "targetPort": 80,
        "cdn": true,
        "customDomains": ["PRD-test.liferay.com"]
      }
    }
  }
}